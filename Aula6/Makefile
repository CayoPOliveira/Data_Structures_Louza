CFLAGS = -Wall -Werror

all: exemplo1

exemplo1: exemplo1.c ArvoreBinaria.o
	gcc $^ -o $@

exemplo2: exemplo2.c 
	gcc $^ -o $@

#regra gen√©rica
%.o: %.c %.h
	gcc $(CFLAGS) -c $<

clean:
	rm -rf *.o exemplo1 exemplo2 exemplo3

valgrind1: exemplo1
	valgrind --leak-check=yes ./exemplo1

valgrind2: exemplo2
	valgrind --leak-check=yes ./exemplo2
